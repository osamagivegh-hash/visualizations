{
  "version": 2,
  "builds": [
    {
      "src": "app_vercel.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app_vercel.py"
    }
  ],
  "functions": {
    "app_vercel.py": {
      "runtime": "python3.9"
    }
  },
  "env": {
    "PYTHON_VERSION": "3.9"
  }
}
